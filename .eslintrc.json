{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "comma-dangle": ["error", "never"],
      "curly": ["error", "multi-or-nest"],
      "func-names": 0,
      "indent": ["error", 2, {"SwitchCase": 1}]
    }
}